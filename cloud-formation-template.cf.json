        "Extension": {
            "Type": "Number"
        },
        "InstallType": {
            "Type": "String"
        },
        "StagingActivateJsonURL": {
            "Type": "String"
        },
        "Key": {
            "Type": "String"
        },
        "VpcId": {
            "Type": "String"
        },
        "SubnetID": {
            "Type": "String"
